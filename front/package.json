  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
    "node": ">= 18.0.0 < 19.0.0"
  },
  "webtoolsManager": {
    "tools": {
      "@appfabric/plugin-cli": "4.x"
    }
  },
  "scripts": {
    "align": "plugin-cli align",
    "prettify": "prettier --write --loglevel warn --ignore-unknown .",
    "prettier-check": "prettier --check --loglevel warn --ignore-unknown .",
    "lint": "plugin-cli lint",
    "lint:ci": "yarn prettier-check && yarn plugin-cli lint",
    "build": "plugin-cli build",
    "insights": "plugin-cli insights",
    "serve": "plugin-cli serve",
    "test": "plugin-cli test:jest && yarn plugin-cli test:cypress",
    "test:jest": "plugin-cli test:jest",
    "test:cypress": "plugin-cli test:cypress",
    "test:cypress:remote": "plugin-cli test:cypress --remote",
    "test:lighthouse": "plugin-cli test:lighthouse --verify",
    "prerelease": "plugin-cli build -p",
    "release": "plugin-cli release",
    "verify-dev-setup": "npx --yes -p @appfabric/scripts verify-dev-setup",
    "awm:bootstrap": "is-ci || (appf-webtools-mgr version && appf-webtools-mgr bootstrap) || echo 'Skipping bootstrap since webtools manager is not installed: https://github.intuit.com/pages/UX-Infra/webtools-mgr/docs/getting-started'",
    "postinstall": "is-ci || (husky install && yarn awm:bootstrap)"
  },
