{
  "name": "@olorin/shared-services",
  "version": "2.0.0",
  "description": "Shared service utilities for the Olorin.ai ecosystem",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "axios": "^1.7.0",
    "tsup": "^8.5.0",
    "typescript": "^5.7.0"
  },
  "license": "UNLICENSED",
  "private": true
}
