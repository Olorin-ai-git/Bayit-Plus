{
  "name": "bayit-plus-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "webpack serve --mode development --config webpack.config.cjs",
    "build": "webpack --mode production --config webpack.config.cjs",
    "dev:vite": "vite",
    "build:vite": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "deploy": "npm run build && firebase deploy --only hosting:web",
    "deploy:preview": "npm run build && firebase hosting:channel:deploy preview --only web",
    "build:webos": "TARGET=webos webpack --mode production --config webpack.config.cjs",
    "package:webos": "npm run build:webos && cd ../webos && ./package-webos.sh",
    "deploy:webos": "npm run package:webos && ares-install -d lg-tv ../webos/*.ipk",
    "launch:webos": "ares-launch -d lg-tv com.bayitplus.app",
    "debug:webos": "ares-inspect -d lg-tv com.bayitplus.app",
    "build:tizen": "TARGET=tizen webpack --mode production --config webpack.config.cjs",
    "package:tizen": "npm run build:tizen && cd ../tizen && ./package-tizen.sh",
    "deploy:tizen": "npm run package:tizen && ~/tizen-studio/tools/ide/bin/tizen install -n ../tizen/Bayit+.wgt -s 192.168.1.165:26101",
    "launch:tizen": "~/tizen-studio/tools/ide/bin/tizen run -p AprZAARz4r.BayitPlus -s 192.168.1.165:26101",
    "build:tv": "npm run build:webos && npm run build:tizen"
  },
  "dependencies": {
    "@noriginmedia/norigin-spatial-navigation": "^2.3.0",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.2.2",
    "@tanstack/react-query": "^5.62.0",
    "axios": "^1.6.2",
    "clsx": "^2.1.1",
    "date-fns": "^3.0.6",
    "date-fns-tz": "^3.2.0",
    "expo-linear-gradient": "^15.0.8",
    "hls.js": "^1.4.14",
    "i18next": "^25.7.3",
    "livekit-client": "^2.7.0",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.1",
    "react-native": "^0.76.9",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-web": "^0.19.13",
    "react-native-web-linear-gradient": "^1.1.2",
    "react-router-dom": "^6.21.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^10.0.0",
    "babel-plugin-react-native-web": "^0.21.2",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.5",
    "postcss": "^8.4.32",
    "postcss-loader": "^8.2.0",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "url-loader": "^4.1.1",
    "vite": "^5.0.10",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
