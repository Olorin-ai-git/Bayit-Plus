{
  "name": "@olorin/shared-email",
  "version": "2.0.0",
  "description": "Olorin Email - Shared email types and interfaces for the Olorin ecosystem",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tracking": {
      "types": "./dist/tracking.d.ts",
      "import": "./dist/tracking.mjs",
      "require": "./dist/tracking.js"
    },
    "./campaign": {
      "types": "./dist/campaign.d.ts",
      "import": "./dist/campaign.mjs",
      "require": "./dist/campaign.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "import": "./dist/preview.mjs",
      "require": "./dist/preview.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
