{
  "name": "@olorin/shared-stores",
  "version": "1.0.0",
  "description": "Shared Zustand store utilities for the Olorin.ai ecosystem",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.9",
    "tsup": "^8.5.0",
    "typescript": "^5.7.0",
    "zustand": "^5.0.0"
  },
  "license": "UNLICENSED",
  "private": true
}
