# ============================================
# PAYMENT FLOW (MANDATORY SUBSCRIPTION)
# ============================================

# [OPTIONAL] Require payment during signup (mandatory subscription model)
# When true: New users must complete payment before accessing app
# When false: Users can access as "viewer" without payment (legacy model)
# Default: false (safe default - disabled until explicitly enabled)
REQUIRE_PAYMENT_ON_SIGNUP=false

# [OPTIONAL] Gradual rollout percentage (0-100)
# Controls what percentage of new signups are required to pay
# 0 = disabled (same as REQUIRE_PAYMENT_ON_SIGNUP=false)
# 5 = 5% of new signups require payment (gradual testing)
# 25 = 25% of new signups require payment
# 100 = all new signups require payment (full rollout)
# Uses hash-based bucketing for consistent user assignment
# Default: 0
REQUIRE_PAYMENT_ON_SIGNUP_PERCENTAGE=0

# [OPTIONAL] Free trial period for new subscriptions (days)
# New users get this many days free after signup payment
# Default: 7
SIGNUP_TRIAL_PERIOD_DAYS=7

# [OPTIONAL] Frontend base URL for payment redirects
# Must match your deployed frontend URL
# Default: http://localhost:3000
FRONTEND_URL=https://bayit.tv

# [OPTIONAL] Frontend path for successful payment redirect
# Default: /payment/success
PAYMENT_SUCCESS_PATH=/payment/success

# [OPTIONAL] Frontend path for cancelled payment redirect
# Default: /payment/cancelled
PAYMENT_CANCELLED_PATH=/payment/cancelled

# [OPTIONAL] Frontend polling interval for payment status (milliseconds)
# How often frontend checks if payment is completed
# Default: 5000 (5 seconds)
PAYMENT_STATUS_POLL_INTERVAL_MS=5000

# [OPTIONAL] Cleanup abandoned signups after N days
# Delete users who created account but never completed payment
# Default: 7
PAYMENT_PENDING_CLEANUP_DAYS=7

# [OPTIONAL] Stripe checkout session expiration (hours)
# How long checkout URLs remain valid
# Default: 24 (Stripe default)
PAYMENT_CHECKOUT_SESSION_TTL_HOURS=24

# [OPTIONAL] Batch size for viewer migration script
# Number of users to migrate at once
# Default: 100
VIEWER_MIGRATION_BATCH_SIZE=100

# [OPTIONAL] Payment conversion threshold for rollback trigger
# If conversion rate falls below this, consider rollback
# Default: 0.40 (40%)
PAYMENT_CONVERSION_THRESHOLD=0.40

